{
  "manifest_version": 2,
  "name": "Search History for GitHub",
  "version": "1.0.1",
  "description": "Search browser history for GitHub URL. Just type \"git\" and press tab in Chrome's address bar.",
  "permissions": [
    "history"
  ],
  "omnibox": {
    "keyword": "git"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "homepage_url": "https://github.com/FujiHaruka/chrome-extention-search-history-for-github",
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  }
}
